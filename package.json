{
  "name": "DerTyp7214-Web-Search",
  "main": "./lib/dertyp7214-web-search",
  "version": "1.86.0",
  "description": "WebSite shortcuts.",
  "keywords": [],
  "repository": "https://github.com/dertyp7214/web-search",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "querystring": "^0.2.0",
    "websocket": "^1.0.24"
  },
  "configSchema": {
    "envUrlLocal": {
      "description": "Local URL for the `env.json`",
      "type": "string",
      "default": "Local"
    },
    "envUrlPublic": {
      "description": "URL for the `env.json`",
      "type": "string",
      "default": "Public"
    },
    "envUrlClean": {
      "description": "Clean URL for the `env.json`",
      "type": "string",
      "default": "Clean"
    },
    "sitesJson": {
      "description": "JSONArray of the websites.",
      "type": "string",
      "default": "google.com, github.com, stackoverflow.com, npmjs.com"
    },
    "showTitles": {
      "description": "Show Titles of the WebSites.",
      "type": "boolean",
      "default": true
    }
  }
}
